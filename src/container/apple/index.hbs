	<footer class="footer">

		{{#each footer}}
		<ul>
			<h1 class="footer_title">{{title}}</h1>
			{{#each links}}
			<li><a href="{{url}}" class="footer_item click">{{text}}</a></li>
			{{/each}}
		</ul>
		{{/each}}

	</footer>
</div>



